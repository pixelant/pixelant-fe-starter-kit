<nav class="main-navigation js__main-navigation navbar navbar-default pxa-main-nav _main-nav-extended_ js__main-nav-extended {{class}} {{#is this.toggleBtn "true"}}pxa-main-nav-root-btn{{/is}}" role="navigation">
<div class="container">

{{#is this.toggleBtn "true"}}
    <button type="button" class="navbar-toggle pxa-nav-toggle-button _animate-to-x_ js__pxa-nav-toggle-button collapsed {{class}}" data-toggle="collapse" data-target="#pxa-root-main-navbar-collapse">
        <span>toggle menu</span>
    </button>
{{/is}}

<div class="main-navigation__items-wrp js__-navigation__items-wrp  collapse navbar-collapse" id="{{#is this.toggleBtn "false"}}pxa-main-navbar-collapse{{/is}}{{#is this.toggleBtn "true"}}pxa-root-main-navbar-collapse{{/is}}">

{{#is this.searchForm "true"}}
    <div class="navbar-form navbar-right main-nav-search-extended">
        <form class="search" role="search" action="search.html">
            <span class="icons icon-t3-search"></span>
            <div class="form-group" id="the-basics">
                <input type="search" class="form-control typeahead quicksearch" placeholder="Search" >
                <button type="submit" style="display:none"></button>
            </div>
        </form>
    </div>
{{/is}}

<ul class="main-navigation__items-list js__main-navigation__items-list nav navbar-nav">

{{#withSort pages "data.number"}}
{{#is data.hideInMenu false}}
{{#is data.menuLevel 1}}
{{#is data.subpages true}}
<li class="main-navigation__item js__main-navigation__item {{#is ../../../../this.dropdown "true"}}_sub {{/is}}{{#is ../../../../page.title data.title}}_active{{/is}}
{{#each ../../../../pages }}
{{#is data.menuLevel 2}}
{{#is ../../../../this.data.title data.parent}}
{{#is ../../../../../../../page.title  data.title}}dropdown _active{{/is}}
{{/is}}
{{/is}}
{{/each}}
{{#each ../../../../pages }}
{{#is data.menuLevel 3}}
{{#is ../../../../this.data.title data.parent}}
{{#is ../../../../../../../page.title  data.title}}dropdown _active{{/is}}
{{/is}}
{{/is}}
{{/each}}
{{#each ../../../../pages }}
{{#is data.menuLevel 4}}
{{#is ../../../../this.data.title data.parent}}
{{#is ../../../../../../../page.title  data.title}}dropdown _active{{/is}}
{{/is}}
{{/is}}
{{/each}}
{{#each ../../../../pages }}
{{#is data.menuLevel 5}}
{{#is ../../../../this.data.title data.parent}}
{{#is ../../../../../../../page.title  data.title}}dropdown _active{{/is}}
{{/is}}
{{/is}}
{{/each}}">
{{!-- <a  href="{{basename}}.html" class="dropdown-toggle-clickable" >{{data.title}}<span class="icons icon-t3-dropdown-arrow"></span></a><a href="{{basename}}.html"  class="icons icon-t3-dropdown-arrow__mobile js__mobile-dropdown-link" ></a> --}}

<a  href="{{basename}}.html" class="main-navigation__item-link dropdown-toggle-clickable" >{{data.title}}</a>
<a href="{{basename}}.html" class="main-navigation__item-link-mobile js__main-navigation__item-link-mobile icons icon-t3-dropdown-arrow__mobile js__mobile-dropdown-link"></a>


<ul class="main-navigation__sub-item-list dropdown-menu">
{{#withSort ../../../../pages "data.number"}}
{{#is data.menuLevel 2}}
{{#is ../../../../this.data.title data.parent}}
<li class="main-navigation__sub-item {{#is ../../../../../../../page.title  data.title}}_active{{/is}}
{{#each ../../../../../../../pages }}
{{#is data.menuLevel 3}}
{{#is ../../../../this.data.title data.parent2}}
{{#is ../../../../../../../../../../page.title  data.title}}_active{{/is}}
{{/is}}
